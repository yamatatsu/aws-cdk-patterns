{
  "name": "private-cloudfront-amplify-front",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "start-auth": "parcel start auth/index.html",
    "prebuild:auth": "rm -rf build/auth/*",
    "build": "yarn build:docusaurus && yarn build:auth",
    "build:auth": "parcel build auth/index.html --public-url ./ --out-dir build/auth",
    "build:docusaurus": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.54",
    "@docusaurus/preset-classic": "^2.0.0-alpha.54",
    "aws-amplify": "^3.0.10",
    "aws-amplify-react": "^4.1.9",
    "classnames": "^2.2.6",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "parcel": "^1.12.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
