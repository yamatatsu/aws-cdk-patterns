{
  "name": "private-cloudfront-amplify-front",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "start-auth": "parcel start auth/index.html",
    "prebuild:auth": "rm -rf build/auth/*",
    "build": "yarn build:docusaurus && yarn build:auth",
    "build:auth": "parcel build auth/index.html --public-url ./ --out-dir build/auth",
    "build:docusaurus": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.ffe8b6106",
    "@docusaurus/preset-classic": "2.0.0-alpha.ffe8b6106",
    "aws-amplify": "3.3.26",
    "aws-amplify-react": "5.0.0",
    "classnames": "2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "parcel": "1.12.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
